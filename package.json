{
  "name": "StorePOS",
  "version": "0.2.2",
  "description": "StorePOS of Sale is a desktop application developed by Monther Tuwati",
  "author": "Monther Tuwati",
  "private": true,
  "main": "start.js",
  "build": {
    "appId": "storepos.app",
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "mas",
      "type": "distribution",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "category": "public.app-category.business",
      "icon": "iconbuilder.icns"
    },
    "linux": {
      "category": "Office",
      "icon": "assets/default.jpg"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "assets/images/StorePOS.png"
    },
    "files": [
      "**/*",
      "assets/**/*",
      "node_modules/**/*",
      "public/**/*"
    ]
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "async": "^3.2.5",
    "better-sqlite3": "^12.2.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "btoa": "^1.2.1",
    "chart.js": "^4.4.3",
    "dotenv": "^16.0.3",
    "electron-context-menu": "^3.6.1",
    "electron-log": "^4.4.8",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "electron-winstaller": "^5.3.1",
    "express": "^4.16.2",
    "express-fileupload": "^1.1.6",
    "feather-icons": "^4.29.2",
    "html2canvas": "^1.0.0-rc.5",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "is-port-reachable": "^4.0.0",
    "jquery": "^3.7.1",
    "jsbarcode": "^3.11.0",
    "jspdf": "^2.3.1",
    "macaddress": "^0.5.3",
    "moment": "^2.24.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "notiflix": "^3.2.6",
    "path": "^0.12.7",
    "print-js": "^1.0.63",
    "simple-keyboard": "^3.7.93",
    "socket.io": "^4.7.5",
    "stripe": "^12.3.0",
    "sweetalert2": "^11.12.0",
    "xmlhttprequest": "^1.8.0"
  },
  "scripts": {
    "start": "electron start.js",
    "electron": "electron start.js",
    "electron-build": "electron-builder",
    "dist:mac": "electron-builder --mac --universal",
    "dist:linux": "electron-builder --linux",
    "package-win": "electron-builder --win",
    "package-win:nsis": "electron-builder --win nsis",
    "package-win:zip": "electron-builder --win zip"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": ">=31.0.2",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "electron-rebuild": "^3.2.9",
    "nodemon": "^1.19.3"
  }
}
